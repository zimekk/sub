{
  "name": "sub",
  "version": "1.0.0-alpha",
  "main": "index.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "scripts": {
    "start": "node src/server",
    "dev--": "ls -la node_modules/.bin && browserlist && nodemon src/server",
    "dev": "nodemon --watch src/server src/server",
    "dev-": "nodemon -e js,jsx --ignore lib --ignore dist --watch src/server --exec babel-node src/server",
    "build": "webpack --output-path=build",
    "predeploy": "yarn build",
    "deploy": "scp -r dist/* root@makarewicz.eu:/var/www/subs/",
    "update": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.50",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "babel-loader": "^8.1.0",
    "convict": "^6.0.0",
    "express": "^4.17.1",
    "graphql-yoga": "^1.18.3",
    "history": "^4.10.1",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "subscriptions-transport-ws": "^0.9.16",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "webpack-dev-middleware": "^3.7.2"
  }
}
